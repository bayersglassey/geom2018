jump:

    if:
        allnocoll:
            ;;       \*/*
            ;;        + -
            ;;       /*\*
            ;; ( )
        allcoll:
            ;; ( )  + - +
    then:
        move: 1 0 0 0
        goto: stand

    if:
        allnocoll:
            ;;       \*/*
            ;;        + -
            ;;       /*\*
            ;; ( )
    then:
        move: 1 0 0 0

    if:
    then:
        die

