
stand:

    if:
        k_forw
        allcoll:
            ;;        +
            ;;       /
            ;; ( )  +
    then:
        move: 1 0 0 0
        rot: 1

    if:
        k_forw
        k_up
        allnocoll:
            ;;       \*/*
            ;;        + -
            ;;       /*\*
            ;; ( )
        anynocoll:
            ;; ( )  + - +
    then:
        move: 1 0 0 0
        goto: jump

    if:
        k_forw
        allcoll:
            ;; ( )  + - +
        allnocoll:
            ;;       \*/*
            ;;        + -
            ;;       /*\*
            ;; ( )
    then:
        move: 1 0 0 0

    if:
        k_forw
        allcoll:
            ;; ( )  +
            ;;       \
            ;;        +
        allnocoll:
            ;;       \*/*
            ;;        + -
            ;;       /*\*/*
            ;; ( )    - + -
            ;;        */*\*
    then:
        move: 1 0 0 0
        rot: -1

    if:
        k_back
    then:
        move: 1 0 0 0
