
prismels:
    sq:
        #     +   +
        # ++ +++ +++ ++   +                             +
        # X+  X   +. ++. +++.  + .   .   .  .  .   .+  X++
        #                 +   +++  ++   +   +  ++  +++  +
        #                      +   ++  +++ +++ ++   +
        #                               +   +
        images:
            : ( 0 -2  2) ( 0 -1  2)
            : ( 0 -3  1) (-1 -2  3) ( 0 -1  1)
            : (-1 -3  1) (-2 -2  3) (-1 -1  1)
            : (-2 -2  2) (-2 -1  2)
            : (-2 -2  1) (-3 -1  3) (-2  0  1)
            : (-2 -1  1) (-3  0  3) (-2  1  1)
            : (-2  0  2) (-2  1  2)
            : (-1  0  1) (-2  1  3) (-1  2  1)
            : ( 0  0  1) (-1  1  3) ( 0  2  1)
            : ( 0  0  2) ( 0  1  2)
            : ( 1 -1  1) ( 0  0  3) ( 1  1  1)
            : ( 1 -2  1) ( 0 -1  3) ( 1  0  1)
    tri:
        #
        #    + ++ +
        # X+ X  . +. ++. + .   .  .  . . .  .+
        #                +   ++  +     + ++  +
        #                        +  ++ +
        images:
            : ( 0 -1  2)
            : ( 0 -2  1) ( 0 -1  1)
            : ( 0 -1  2)
            : ( 0 -2  1) ( 0 -1  1)
            : ( 0 -1  2)
            : ( 0 -2  1) ( 0 -1  1)
            : ( 0 -1  2)
            : ( 0 -2  1) ( 0 -1  1)
            : ( 0 -1  2)
            : ( 0 -2  1) ( 0 -1  1)
            : ( 0 -1  2)
            : ( 0 -2  1) ( 0 -1  1)

shapes:
    sixth:
        prismels:
            : tri (0 0 0 0)  0 f 0
            : tri (1 0 0 0) 11 f 1
            : sq  (1 0 0 0)  1 f 2
    dodeca:
        shapes:
            : sixth (0 0 0 0)  0 f
            : sixth (0 0 0 0)  2 f
            : sixth (0 0 0 0)  4 f
            : sixth (0 0 0 0)  6 f
            : sixth (0 0 0 0)  8 f
            : sixth (0 0 0 0) 10 f

