
unit: 3 2 0 -1
spawn: "data/maps/demo/start.fus"
palette: "data/pal1.fus"
tileset: "data/tileset1.fus"

actors:
    : "actor/guide/actor.fus" "player_cpu"
    : "actor/eyeplayer_tunnels.fus"
    : "actor/eyeplayer_dodecas.fus"
    : "actor/eyeplayer_vines.fus"

vars:
    "guide_passed_start": F
    "guide_passed_start2": F

    "eyeplayer_tunnels_ran_away_1": F
    "eyeplayer_dodecas_ran_away_1": F
    "eyeplayer_vines_ran_away_1": F
    "eyeplayer_vines_ran_away_2": F
    "eyeplayer_vines_ran_away_3": F
    "eyeplayer_vines_mini_eye_respawn": 0

submaps:
    :
        file: "data/maps/demo/start.fus"
        pos: (0 0)
        camera: (6 4)
        mapper: ("quadruple")
        palette: "data/maps/demo/pals/start.fus"
    :
        file: "data/maps/demo/start2.fus"
        pos: (7 11)
        camera: (5 4)
        mapper: ("quadruple")
