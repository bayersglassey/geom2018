
# Expected vars:
# CRAWL_SPIT (str) e.g. "crawl_spit", "crawl_looking_up_spit"

# Spit
if:
    key: wentdown y
then:
    call: $GET_STR CRAWL_SPIT

# Turn into bird
if:
    expr: myvar("is_birdspider")
    key: wentdown x
    coll: all no
        ;;   .   .
        ;;   *\*/*
        ;; . - + - .
        ;;   */*\*
        ;;   . - .
        ;;   *\*/*
        ;; . - + - .
        ;;   */*\*
        ;;  (.)  .
then:
    goto: "bird_crawl"
    delay: 1
